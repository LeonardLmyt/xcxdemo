/*小程序主色调*/
page{
  --c:#0DBEC8;
}
.c{color:var(--c);}
.bg{background-color:var(--c);}
.b{border:1px solid var(--c);}

/*footer*/
.footer{position: fixed;bottom: 0;left: 0;right: 0;height: 115rpx;}
.footer_sub{display: -webkit-box;display:box;width: 100%;height: 100%;-webkit-box-align: center;box-align: center;background-color: #fff;}
.footer_tab{display: -webkit-box;display:box;-webkit-box-flex: 1.0;box-flex: 1.0;-webkit-box-pack: center;box-pack: center;width: 1%;-webkit-box-orient: vertical;box-orient: vertical;-webkit-box-align: center;box-align: center;background-color: #ffffff;}
.footer_tab view:first-child{width: 60rpx;height: 60rpx;margin-bottom: 10rpx;background-size: 100% 100%;-webkit-background-size: 100% 100%;}
.footer_tab:nth-child(1) view:first-child{background-image:url(data:image/png;base64,)}
.footer_tab:nth-child(2) view:first-child{background-image:url(data:image/png;base64,)}
.footer_tab view:last-child{color: #9A9A9A;font-size: 0.8em;}


/*layer*/
.layer{position: fixed;top: 40%;left: 0;right: 0;width: 100%;overflow: hidden;pointer-events: none;z-index: 9999;}
.layer-sub{display:-webkit-box;display:box;width: 100%;overflow: hidden;}
.layer-sub view{-webkit-box-flex: 1.0;box-flex: 1.0;}
.layer-content{-webkit-box-flex-group: 2;flex-group: 2;background-color: rgba(0,0,0,.6);color: #fff;padding: 20rpx 30rpx;border-radius: 10rpx;-webkit-border-radius: 10rpx;height: 40rpx;line-height: 40rpx;border: 1px solid #000;}
.layer-warning{animation: layerErrorAnimation 1s;-webkit-animation: layerErrorAnimation 1s;color: #8a6d3b;background-color: #fcf8e3;border: 1px solid #faebcc;}
.layer-error{animation: layerErrorAnimation 1s;-webkit-animation: layerErrorAnimation 1s;color: #a94442;background-color: #f2dede;border: 1px solid #ebccd1;}
 @keyframes layerErrorAnimation {0%, 100% {-webkit-transform: translateX(0);}10%,30%,50%,70%,90% {-webkit-transform: translateX(-5px);}20%,40%,60%,80% {-webkit-transform: translateX(5px);}} 
@-webkit-keyframes layerErrorAnimation {0%, 100% {-webkit-transform: translateX(0);}10%,30%,50%,70%,90% {-webkit-transform: translateX(-5px);}20%,40%,60%,80% {-webkit-transform: translateX(5px);}}